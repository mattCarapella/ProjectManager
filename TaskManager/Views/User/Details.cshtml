                                                <td>@Html.DisplayFor(modelItem => ticket.Ticket.Status)</td>
                                                <td>@Html.DisplayFor(modelItem => ticket.Ticket.TicketType)</td>
                                                <td>@Html.DisplayFor(modelItem => ticket.Ticket.Priority)</td>
                                                <td>@Html.DisplayFor(modelItem => ticket.Ticket.GoalDate)</td>
                                                @*<td><a asp-controller="Tickets" asp-action="Details" asp-route-id="@ticket.TicketId">Details</a></td>*@
                                            </tr>
                                        }
                                    </table>
                                </div>


                                
