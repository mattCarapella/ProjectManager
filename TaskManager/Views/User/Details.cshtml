@{
    ViewData["Title"] = "User Details";
}
@model TaskManager.Core.ViewModels.UserViewModel

<div><a href="javascript:history.go(-1)">Back</a></div>

<div class="main-container">
    <div class="container pb-2">
        <div class="row justify-content-center">
            <div class="col-xl-12 col-lg-12">

                <div class="page-header mb-2">

                    <div class="wrapper">
                        <div class="media">
                            <img alt="Image" src="~/images/@Model.User.ProfilePicture" class="profile-pic avatar avatar-lg mt-1" overflow="hidden" />
                            <div class="media-body ml-3">
                                <h1 class="mb-0">@Model.User.FullName</h1>
                                <p class="lead">@Model.User.JobTitle</p>
                            </div>
                        </div>
                        <span class="center-vert">
                            <button class="btn btn-primary">
                                <a asp-controller="User" asp-action="Edit" asp-route-id="@Model.User.Id" class="btn-cstm-link-text-light">Edit Details</a>
                            </button>
                        </span>
                    </div>
                </div>
                <div>
                    @***** USER DETAILS *****@
                    <hr />
                    <dl class="row">
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.User.UserName)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.User.UserName)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.User.Email)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.User.Email)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.User.JobTitle)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.User.JobTitle)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.User.EmployeeID)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.User.EmployeeID)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.User.CreatedAt)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.User.CreatedAt)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.User.LastLoggedInAt)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.User.LastLoggedInAt)
                        </dd>
                        <dt class="col-sm-2">
                            Roles
                        </dt>
                        <dd class="col-sm-10">
                            <ul>
                                @foreach (var role in @Model.Roles)
                                {
                                    <li>@role</li>
                                }
                            </ul>
                        </dd>

                    </dl>
                </div>
            </div>
        </div>
    </div>


